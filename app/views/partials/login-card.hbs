<div class="uk-width-3-3@m  uk-card uk-card-default uk-padding-small uk-visible@m">
  <div class="uk-grid">
    <div class="uk-width-1-5@m">
      <img src="/images/ireland1.png" style="margin-left: 4em" width="60" alt="img" />
    </div>
    <div class="uk-width-3-5@m uk-text-emphasis uk-text-left">
      <div class="title uk-text-large">
        {{title}}
      </div>
      <div class="uk-text-muted">
        {{subTitle}}
      </div>
    </div>
    <div class="uk-width-1-5@m">
      {{>welcome-menu}}
    </div>
  </div>
</div>
